<svg width="600" height="400" viewBox="0 0 600 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="600" height="400" fill="url(#pictureGradient)"/>
  
  <!-- AI Generation Studio -->
  <rect x="30" y="30" width="540" height="340" rx="20" fill="rgba(255,255,255,0.95)" stroke="rgba(16,185,129,0.2)" stroke-width="2"/>
  
  <!-- Central AI Brain -->
  <circle cx="300" cy="200" r="60" fill="url(#aiGradient)" opacity="0.9"/>
  <circle cx="300" cy="200" r="45" fill="none" stroke="rgba(139,92,246,0.6)" stroke-width="3" stroke-dasharray="10,5"/>
  <circle cx="300" cy="200" r="30" fill="none" stroke="rgba(59,130,246,0.8)" stroke-width="2" stroke-dasharray="5,3"/>
  
  <!-- Neural Network Lines -->
  <g stroke="rgba(139,92,246,0.4)" stroke-width="2" opacity="0.7">
    <line x1="240" y1="160" x2="280" y2="180"/>
    <line x1="320" y1="180" x2="360" y2="160"/>
    <line x1="240" y1="240" x2="280" y2="220"/>
    <line x1="320" y1="220" x2="360" y2="240"/>
    <line x1="260" y1="140" x2="290" y2="170"/>
    <line x1="310" y1="170" x2="340" y2="140"/>
    <line x1="260" y1="260" x2="290" y2="230"/>
    <line x1="310" y1="230" x2="340" y2="260"/>
  </g>
  
  <!-- Input Text Prompt -->
  <rect x="80" y="80" width="200" height="80" rx="15" fill="rgba(59,130,246,0.1)" stroke="rgba(59,130,246,0.3)" stroke-width="2"/>
  <text x="180" y="100" text-anchor="middle" fill="#3B82F6" font-family="Arial" font-size="10" font-weight="bold">TEXT PROMPT</text>
  <rect x="90" y="110" width="180" height="6" rx="3" fill="#3B82F6" opacity="0.7"/>
  <rect x="90" y="120" width="160" height="4" rx="2" fill="#6B7280"/>
  <rect x="90" y="130" width="140" height="4" rx="2" fill="#6B7280"/>
  <rect x="90" y="140" width="120" height="4" rx="2" fill="#6B7280"/>
  
  <!-- Generated Images Gallery -->
  <!-- Image 1 - Landscape -->
  <rect x="420" y="70" width="120" height="80" rx="12" fill="url(#image1Gradient)" stroke="rgba(16,185,129,0.5)" stroke-width="2"/>
  <circle cx="450" cy="100" r="8" fill="rgba(251,191,36,0.9)"/> <!-- Sun -->
  <path d="M430 130 Q460 110 490 130 Q480 140 470 135 Q450 140 440 135 Q430 140 430 130" fill="rgba(34,197,94,0.8)"/> <!-- Hills -->
  <rect x="435" y="135" width="50" height="10" fill="rgba(59,130,246,0.6)"/> <!-- Water -->
  
  <!-- Image 2 - Portrait -->
  <rect x="420" y="170" width="120" height="80" rx="12" fill="url(#image2Gradient)" stroke="rgba(139,92,246,0.5)" stroke-width="2"/>
  <circle cx="480" cy="195" r="12" fill="rgba(92,400,14,0.8)"/> <!-- Face -->
  <rect x="468" y="210" width="24" height="30" rx="12" fill="rgba(92,400,14,0.8)"/> <!-- Body -->
  <rect x="440" y="230" width="80" height="15" rx="8" fill="rgba(59,130,246,0.3)"/> <!-- Background -->
  
  <!-- Image 3 - Abstract -->
  <rect x="420" y="270" width="120" height="80" rx="12" fill="url(#image3Gradient)" stroke="rgba(245,158,11,0.5)" stroke-width="2"/>
  <circle cx="450" cy="300" r="15" fill="rgba(239,68,68,0.7)"/>
  <circle cx="490" cy="290" r="10" fill="rgba(59,130,246,0.8)"/>
  <circle cx="470" cy="325" r="12" fill="rgba(16,185,129,0.7)"/>
  <path d="M440 280 Q480 270 520 290 Q500 320 460 330 Q440 320 440 280" fill="rgba(139,92,246,0.3)"/>
  
  <!-- AI Processing Animation -->
  <g opacity="0.8">
    <circle cx="220" cy="200" r="4" fill="#3B82F6">
      <animate attributeName="r" values="4;8;4" dur="2s" repeatCount="indefinite"/>
      <animate attributeName="opacity" values="0.8;0.3;0.8" dur="2s" repeatCount="indefinite"/>
    </circle>
    <circle cx="380" cy="200" r="4" fill="#10B981">
      <animate attributeName="r" values="4;8;4" dur="2.5s" repeatCount="indefinite"/>
      <animate attributeName="opacity" values="0.8;0.3;0.8" dur="2.5s" repeatCount="indefinite"/>
    </circle>
  </g>
  
  <!-- Style Options -->
  <rect x="80" y="240" width="200" height="100" rx="15" fill="rgba(16,185,129,0.1)" stroke="rgba(16,185,129,0.3)" stroke-width="2"/>
  <text x="180" y="260" text-anchor="middle" fill="#10B981" font-family="Arial" font-size="10" font-weight="bold">STYLE OPTIONS</text>
  
  <rect x="90" y="275" width="35" height="20" rx="4" fill="rgba(59,130,246,0.2)" stroke="rgba(59,130,246,0.4)"/>
  <text x="107" y="287" text-anchor="middle" fill="#3B82F6" font-family="Arial" font-size="8">Realistic</text>
  
  <rect x="135" y="275" width="35" height="20" rx="4" fill="rgba(139,92,246,0.2)" stroke="rgba(139,92,246,0.4)"/>
  <text x="152" y="287" text-anchor="middle" fill="#8B5CF6" font-family="Arial" font-size="8">Artistic</text>
  
  <rect x="180" y="275" width="35" height="20" rx="4" fill="rgba(16,185,129,0.2)" stroke="rgba(16,185,129,0.4)"/>
  <text x="197" y="287" text-anchor="middle" fill="#10B981" font-family="Arial" font-size="8">Abstract</text>
  
  <rect x="225" y="275" width="35" height="20" rx="4" fill="rgba(245,158,11,0.2)" stroke="rgba(245,158,11,0.4)"/>
  <text x="242" y="287" text-anchor="middle" fill="#F59E0B" font-family="Arial" font-size="8">Cartoon</text>
  
  <!-- Resolution Options -->
  <rect x="90" y="305" width="180" height="25" rx="8" fill="rgba(55,65,81,0.1)"/>
  <text x="100" y="320" fill="#374151" font-family="Arial" font-size="9">Resolution: 1024x1024 • 4K • HDR</text>
  
  <!-- Magic Wand -->
  <g transform="translate(340, 160)">
    <line x1="0" y1="40" x2="30" y2="10" stroke="#F59E0B" stroke-width="4" stroke-linecap="round"/>
    <circle cx="32" cy="8" r="3" fill="#F59E0B"/>
    <circle cx="28" cy="4" r="2" fill="#FDE047" opacity="0.8"/>
    <circle cx="36" cy="12" r="2" fill="#FDE047" opacity="0.8"/>
    <circle cx="25" cy="15" r="1.5" fill="#FDE047" opacity="0.6"/>
    <circle cx="38" cy="6" r="1.5" fill="#FDE047" opacity="0.6"/>
  </g>
  
  <!-- Progress Bar -->
  <rect x="150" y="350" width="300" height="8" rx="4" fill="rgba(229,231,235,0.8)"/>
  <rect x="150" y="350" width="180" height="8" rx="4" fill="url(#progressGradient)"/>
  <text x="300" y="375" text-anchor="middle" fill="#6B7280" font-family="Arial" font-size="10">Generating... 60%</text>
  
  <defs>
    <linearGradient id="pictureGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#F0FDF4"/>
      <stop offset="100%" stop-color="#FEF3E2"/>
    </linearGradient>
    <radialGradient id="aiGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" stop-color="#8B5CF6" stop-opacity="0.8"/>
      <stop offset="100%" stop-color="#3B82F6" stop-opacity="0.4"/>
    </radialGradient>
    <linearGradient id="image1Gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#DBEAFE"/>
      <stop offset="100%" stop-color="#FEF3C7"/>
    </linearGradient>
    <linearGradient id="image2Gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#F3E8FF"/>
      <stop offset="100%" stop-color="#DBEAFE"/>
    </linearGradient>
    <linearGradient id="image3Gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#FEF3C7"/>
      <stop offset="100%" stop-color="#FECACA"/>
    </linearGradient>
    <linearGradient id="progressGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" stop-color="#10B981"/>
      <stop offset="100%" stop-color="#3B82F6"/>
    </linearGradient>
  </defs>
</svg>